    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-merge": "5.0.9",
    "webrtc-adapter": "4.2.1",
    "ws": "7.3.1",
    "xmlhttprequest": "1.8.0"
  }
