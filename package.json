  "scripts": {
    "preinstall": "npm prune",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.ci.js",
    "test-browsers": "karma start test/karma.conf.browsers.js",
    "test-continuous": "karma start test/karma.conf.js",
    "coverage": "karma start test/karma.conf.coverage.js",
    "check-coverage": "istanbul check-coverage --config=test/coverage.yml",
    "webpack": "webpack",
    "webpack-workflow-example": "webpack --config webpack.example.config",
    "grunt": "grunt -d",
    "bundle": "grunt build -d",
    "ci-test": "node scripts/ci-test.js",
    "ci-bundle": "node scripts/ci-bundle.js",
    "ci-coverage": "node scripts/ci-coverage.js",
    "start": "node example/httpserver.js",
    "jpm-run": "jpm run --addon-dir src/firefox",
    "jpm-xpi": "jpm xpi --addon-dir src/firefox",
    "lint": "eslint ./ â€”-cache",
    "lint-fix": "eslint ./ --fix",
    "prepack": "npm run webpack-workflow-example && npm run add-copyright",
    "add-copyright": "./../phenix/scripts/add-copyright.sh ./",
    "deploy-examples:example": "gsutil -h \"Cache-Control:public, max-age=600\" -m cp -R example/* gs://phenix-assets-us/examples",
    "deploy-examples:src": "gsutil -h \"Cache-Control:public, max-age=600\" -m cp -R src/* gs://phenix-assets-us/examples",
    "deploy-examples:dist": "gsutil -h \"Cache-Control:public, max-age=600\" -m cp -R dist/* gs://phenix-assets-us/examples",
    "deploy-examples:deps": "gsutil -h \"Cache-Control:public, max-age=600\" -m cp -R node_modules/phenix-* node_modules/jquery node_modules/lodash node_modules/bootstrap node_modules/bootstrap-notify node_modules/fingerprintjs2 node_modules/shaka-player node_modules/requirejs node_modules/animate.css gs://phenix-assets-us/examples",
    "deploy-examples": "node scripts/deploy-examples.js"
  },
